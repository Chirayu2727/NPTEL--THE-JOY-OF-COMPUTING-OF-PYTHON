        "def snake(M):\n",
        "  sk=0\n",
        "  sm=[]\n",
        "  for r in M:\n",
        "    m=[]\n",
        "    sk=sk+1\n",
        "    if sk%2==0:\n",
        "      m=r[::-1]\n",
        "    else:\n",
        "      m=r\n",
        "    sm+=m\n",
        "  return(sm)\n",
        "n = int(input())\n",
        "M = []\n",
        "for i in range(n):\n",
        "    L = list(map(int, input().split()))\n",
        "    M.append(L)\n",
        "\n",
        "print(snake(M))"
      ]
