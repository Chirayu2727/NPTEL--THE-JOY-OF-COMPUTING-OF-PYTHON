      "source": [
        "def subStr(s1, s2):\n",
        "    len_s1 = len(s1)\n",
        "    len_s2 = len(s2)\n",
        "\n",
        "    for i in range(len_s1 - len_s2 + 1):\n",
        "        if s1[i:i+len_s2] == s2:\n",
        "            return True\n",
        "\n",
        "    return False"
      ]
